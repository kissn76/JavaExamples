<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="6.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="threads.countdownTimer.CountdownTimerController">
   <padding>
      <Insets bottom="6.0" left="6.0" right="6.0" top="6.0" />
   </padding>
   <children>
      <Label id="countdownLabel" fx:id="countdownLabel" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
      <HBox alignment="CENTER_LEFT" spacing="6.0">
         <children>
            <TextField id="countdownTime" fx:id="countdownTime" alignment="CENTER_RIGHT" prefWidth="100.0" promptText="60" text="60" /> 
            <Label text="sec" />
            <Button id="countdownStartBtn" fx:id="countdownStartBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Start" />
            <Button id="countdownPauseBtn" fx:id="countdownPauseBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Pause" />
         </children>
      </HBox>
   </children>
</VBox>
